  "jshintConfig": {
    "undef": true,
    "unused": true,
    "esversion": 9,
    "node": true,
    "globals": {
      "$": true
    }
  },
  "devDependencies": {},
  "dependencies": {
    "bluebird": "^3.7.2",
    "chalk": "^3.0.0",
    "colors": "^1.4.0",
    "fs": "^0.0.2",
    "jshint": "^2.11.0",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.3",
    "moment": "^2.24.0",
    "octokat": "^0.10.0",
    "tcomb-validation": "^3.4.1",
    "yargs": "^15.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MAARK/github-issue-voyager.git"
  },
  "bugs": {
    "url": "https://github.com/MAARK/github-issue-voyager/issues"
  },
  "homepage": {
    "url": "https://github.com/MAARK/github-issue-voyager"
